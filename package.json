{
  "name": "soft-expert-code-challenge",
  "version": "0.1.0",
  "description": "A Pokedex web app for SoftExpert code challenge",
  "homepage": "http://giovanicascaes.github.io/soft-expert-code-challenge",
  "main": "src/index.jsx",
  "scripts": {
    "serve-webpack": "webpack serve --config webpack.dev.js --hot",
    "build-webpack": "NODE_ENV=production npm run build-css && webpack --config webpack.prod.js",
    "deploy": "gh-pages -d dist",
    "build-css": "postcss src/styles/index.css -o src/assets/css/index.css",
    "watch-css": "postcss -w src/styles/index.css -o src/assets/css/index.css",
    "startAndWatchCss": "concurrently 'npm:watch-css' 'npm:serve-webpack'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "concurrently": "^5.3.0",
    "postcss-cli": "^8.1.0",
    "postcss-import": "^13.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.13.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.2.0",
    "tailwindcss": "^1.9.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^6.0.4",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "eslint": "^7.12.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.1.1",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.2.0",
    "postcss": "^8.1.4",
    "postcss-loader": "^4.0.4",
    "style-loader": "^2.0.0",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "keywords": [
    "react",
    "pokemon",
    "softexpert"
  ],
  "author": "giovanicascaes@gmail.com",
  "license": "MIT"
}
